{
	"cli": {
		"version": ">= 3.13.0"
	},
	"build": {
		"development": {
			"node": "20.17.0",
			"android": {
				"image": "latest"
			},
			"ios": {
				"simulator": true,
				"image": "latest"
			},
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"ANI_ID": "12490",
				"APP_VARIANT": "development"
			}
		},
		"development-simulator": {
			"developmentClient": true,
			"distribution": "internal",
			"ios": {
				"simulator": true
			},
			"env": {
				"ANI_ID": "12490",
				"APP_VARIANT": "development"
			}
		},
		"preview": {
			"distribution": "internal",
			"env": {
				"ANI_ID": "12490",
				"APP_VARIANT": "preview",
				"SENTRY_AUTH_TOKEN": "TestToken"
			},
			"channel": "preview",
			"android": {
				"image": "latest"
			}
		},
		"production": {
			"channel": "production",
			"env": {
				"ANI_ID": "12490",
				"APP_VARIANT": "store"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
