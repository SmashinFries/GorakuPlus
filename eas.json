{
    "cli": {
        "version": ">= 3.13.0"
    },
    "build": {
        "development": {
            "android": {
                "image": "latest"
            },
            "ios": {
                "simulator": true
            },
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "ANI_ID": "12490",
                "APP_VARIANT": "development"
            }
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "ANI_ID": "12490"
            },
            "channel": "preview",
            "android": {
                "image": "latest"
            }
        },
        "production": {
            "channel": "production"
        }
    },
    "submit": {
        "production": {}
    }
}
